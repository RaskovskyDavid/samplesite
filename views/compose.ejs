<%- include('partials/header') -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label  for="composeTitle">Title</label>
        <input class="form-control" type="text" name="composeTitle" autocomplete="off">
    </div>
    <div class="form-group">
        <label for="composePost">Post</label>
        <textarea class="form-control" name="composePost" id="" cols="30" rows="10"></textarea>
    </div>
    <button class="btn btn-primary cbtn" type="submit" >Publish</button>
</form>
<%- include('partials/footer') -%>